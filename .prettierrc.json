{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "bracketSameLine": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "printWidth": 1000
      }
    },
    {
      "files": ["*.css", "*.json"],
      "options": {
        "printWidth": 80
      }
    }
  ]
}
